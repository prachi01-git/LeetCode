bool isCovered(int** ranges, int rangesSize, int* rangesColSize, int left, int right) {
 /*create arr[52]
 assign 0
 start=+1
 ending=-1
 find prefix sum
 generate value from L to R
 compare*/
  int arr[52]={0};
for(int i=0 ; i<rangesSize ; i++){
    int start=ranges[i][0];
    int end=ranges[i][1];
    arr[start]++;
    arr[end+1]--;
}
for(int i=1 ; i<52 ; i++){
    arr[i]=arr[i]+arr[i-1];
}
for(int i=left ; i<=right ; i++){
    if(arr[i]<=0){
        return false;
    }
}
return true;

  }


